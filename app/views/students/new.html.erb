<h1>Students#new</h1>

<%= form_for(@student) do |f|%>
<%= f.label :first_name, "First Name"%>
<%= f.text_field :first_name%><br>
<%= f.label :last_name, "Last Name"%>
<%= f.text_field :last_name%><br>
<%= f.label :education, "Education Level"%>
<%= f.select(:education) do %>
  <% [['GED', 'GED'], ['Highschool'], ['Associates'], ['Bachelors'], ['Masters'], ['Professional']].each do |c| -%>
    <%= content_tag(:option, c.first, value: c.last) %>
  <% end %>
  <% end%><br>
<%= f.label :age%>
<%= f.number_field :age%><br>
<%= f.submit "Submit"%>
<% end %>
<p class="edit-button-back">
   <%= link_to root_path,  { class: 'btn btn-primary goback-button' } do %>
   <i class="fas fa-long-arrow-alt-left"></i> Back
   <% end %>
</p>